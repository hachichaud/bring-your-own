h2 Login

md-content(
  layout-padding
  layout="column"
)
  md-content(
    layout="row"
    layout-sm="column"
  )
    md-input-container
      label Username
      input(ng-model="user.username")
    md-input-container
      label Password
      input(
        ng-model="user.password"
        type="password"
      )
  md-button.md-primary(
    ng-click="logIn(user)"
  ) Log In
